{
  "name": "svc-db-mysql",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "docker:build": "docker-compose -p mysql -f mysql.stack.yml build",
    "_docker:copy": "cp ./initdb.d/* ../svc-storage/var/mysql/initdb.d",
    "docker:up": "yarn _docker:copy && docker-compose -p mysql -f mysql.stack.yml up -d",
    "docker:down": "docker-compose -p mysql -f mysql.stack.yml down"
  }
}
