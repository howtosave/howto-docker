{
  "name": "SVC_DB",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "docker:build": "docker-compose -p mysql -f mysql.stack.yml build",
    "_docker:copy": "cp ./initdb.d/* ../svc-storage/var/mysql/initdb.d",
    "docker:run": "yarn _docker:copy && docker-compose -p mysql -f mysql.stack.yml up -d",
    "docker:kill": "docker-compose -p mysql -f mysql.stack.yml down"
  }
}
