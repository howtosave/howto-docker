{
  "name": "multi-prefix-url",
  "version": "1.0.0",
  "scripts": {
    "docker:init": "mkdir -p ./volume/nginx/readwrite/run;mkdir -p ./volume/nginx/readwrite/log;mkdir -p ./volume/nginx/readwrite/log/{a.com,b.com,c.com}",
    "docker:build": "docker-compose -f stack.yml build",
    "docker:up": "docker-compose -f stack.yml up",
    "docker:down": "docker-compose -f stack.yml down",
    "conf:check": "docker exec -it my-nginx nginx -t",
    "conf:reload": "docker exec -it my-nginx nginx -s reload"
  },
  "license": "MIT"
}
